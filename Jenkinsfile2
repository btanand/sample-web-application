#!/usr/bin/env groovy

@Library('shared-library@master') _ //master or whatever branch

pipeline{
	
	agent any
	
	tools {
        maven 'apache-maven-3.6.3'
       
    	}
        
        stages{
		stage ('Initialize') {
        	    steps {
               		 sh '''
                    		echo "PATH = ${PATH}"
                    		echo "M2_HOME = ${M2_HOME}"
                	    '''
           		 }
		}
              stage('maven build'){
                  steps{
                      script{
		    	                bat "mvn clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 1)
                    }
                }
		
           }	       	     	         
}
